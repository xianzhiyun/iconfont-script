{
  "name": "iconfont-script",
  "version": "1.0.0",
  "description": "iconfont-script 实现下载iconfont网站zip文件，并解压指定目录，避免项目开发前期繁琐操作",
  "main": "dist/iconfont.cjs.js",
  "module": "dist/iconfont.esm.js",
  "browser": "dist/iconfont.umd.js",
  "author": "张乐",
  "homepage": "git@github.com:xianzhiyun/iconfont-script.git",
  "keywords": [
    "iconfont",
    "icon",
    "font",
    "svg",
    "iconfont-",
    "iconfont-script",
    "script",
    "tools",
    "javascript",
    "library"
  ],
  "scripts": {
    "dev:build": "rollup -c -w",
    "dev:start": "nodemon node test/index.js",
    "dev": "concurrently npm:dev:*",
    "test": "node test/index.js",
    "build": "cross-env NODE_ENV=production rollup -c",
    "pretest": "npm run build"
  },
  "dependencies": {
    "compressing": "^1.5.0",
    "core-js": "^2.6.10",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.5.1",
    "nodemon": "^2.0.3",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2"
  },
  "files": [
    "dist"
  ],
  "nodemonConfig": {
    "watch": "test/index.js",
    "ignore": [
      "dist",
      "dist/iconfont*",
      "dist/iconfont.zip",
      "src/*",
      "test/features/*",
      "features"
    ],
    "delay": "2500"
  }
}
